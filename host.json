{
  "version": "2.0",
  "logging": {
    "loglevel": {
      "default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "extensions": {
    "queues": {
      "batchSize": 1,
      "maxDequeueCount": 5,
      "newBatchThreshold": 0
    }
  }
}
